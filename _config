workspace_layout tabbed
floating_modifier $m4

set $m4  Mod4
set $alt Mod1
set $c   Control
set $S   Shift

set $i3 ${XDG_CONFIG_HOME}/i3

set $circle exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send circle
set $ns exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send ns
set $flast exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send flast
set $menu exec ${XDG_CONFIG_HOME}/i3/send menu
set $wm3 exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send wm3
set $volume exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send vol
set $executor exec --no-startup-id ${XDG_CONFIG_HOME}/i3/send executor

set $scratchpad_dialog floating enable, move absolute position 180 20, resize set 1556 620, move scratchpad

